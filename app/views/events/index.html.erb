<h1>Events</h1>


    <% @events.each do |event| %>
      <div class="col-sm-4 col-lg-4 col-md-4">
        <div class="thumbnail">
          <img src="http://placehold.it/320x150" alt="">
          <div class="caption">
            <h4 class="pull-right"><%= event.start_time.strftime("%b %d") %></h4>
            <h4><a href="#"><%= event.name %></a>
            </h4>
            <p><%= event.description %></p>
          </div>
          <div class="ratings">
            <p class="pull-right"><%= link_to 'Show', event %></p>
            <p><%= event.venue %></p>
          </div>
        </div>
      </div>
    <% end %>
  



    
